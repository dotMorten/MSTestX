<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="MSTestX.Styles.DefaultTheme">
    <Color x:Key="accentColor">#FF6495ED</Color>
    <Color x:Key="backgroundColor">#FFFFFFFF</Color>
    <Color x:Key="foregroundColor">#FF000000</Color>
    <Color x:Key="successColor">#FF008000</Color>
    <Color x:Key="notExecutedColor">#FF808080</Color>
    <Color x:Key="failedColor">#FFFF0000</Color>
    <Color x:Key="skippedColor">#FFFFA500</Color>
    <Color x:Key="timeoutColor">#FFF45000</Color>
    <Style x:Key="backgroundStyle" TargetType="VisualElement">
        <Setter Property="BackgroundColor" Value="{DynamicResource backgroundColor}" />
    </Style>
</ResourceDictionary>